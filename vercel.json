{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "dist",
  "functions": {
    "dist/server/entry.ssr.js": {
      "runtime": "nodejs20.x"
    }
  },
  "rewrites": [
    {
      "source": "/assets/(.*)",
      "destination": "/dist/client/$1",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "source": "/(.*)",
      "destination": "/dist/server/entry.ssr.js"
    }
  ]
}
