{
  "version": 2,
  "framework": "vite",
  "buildCommand": "npm run build && npm run build.ssr",
  "outputDirectory": "dist",
  "builds": [
    {
      "src": "dist/server/entry.ssr.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/dist/client/$1",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/dist/server/entry.ssr.js"
    }
  ]
}
